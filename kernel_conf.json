{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${KERNEL_SOURCE_PATH}/include/",
                "${KERNEL_SOURCE_PATH}/include/uapi/",
                "${KERNEL_SOURCE_PATH}/include/generated/uapi/",
                "${KERNEL_SOURCE_PATH}/arch/x86/include/",
                "${KERNEL_SOURCE_PATH}/arch/x86/include/uapi",
                "${KERNEL_SOURCE_PATH}/arch/x86/include/generated/",
                "${KERNEL_SOURCE_PATH}/arch/x86/include/generated/uapi",
                "${KERNEL_SOURCE_PATH}/usr/local/include",
                "${KERNEL_SOURCE_PATH}/usr/include"
            ],
            "defines": [
                "__GNUC__",
                "__KERNEL__",
                "__LINUX_KCONFIG_",
                "CONFIG_AS_CFI=1",
                "CONFIG_AS_CFI_SIGNAL_FRAME=1",
                "CONFIG_AS_CFI_SECTIONS=1",
                "CONFIG_AS_SSSE3=1",
                "CONFIG_AS_AVX=1",
                "CONFIG_AS_AVX2=1",
                "CONFIG_AS_AVX512=1",
                "CONFIG_AS_SHA1_NI=1",
                "CONFIG_AS_SHA256_NI=1",
                "CONFIG_AS_ADX=1"
            ],
            "forcedInclude": [
                "${KERNEL_SOURCE_PATH}/include/linux/kconfig.h",
                "${KERNEL_SOURCE_PATH}/include/linux/compiler-clang.h",
                "${KERNEL_SOURCE_PATH}/include/linux/compiler-gcc.h",
                "${KERNEL_SOURCE_PATH}/include/linux/compiler.h",
                "${KERNEL_SOURCE_PATH}/include/linux/compiler-intel.h",
                "${KERNEL_SOURCE_PATH}/include/linux/intel-iommu.h"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu17",
            "cppStandard": "gnu++20",
            "intelliSenseMode": "gcc-x64"
        }
    ],
    "version": 4
}

