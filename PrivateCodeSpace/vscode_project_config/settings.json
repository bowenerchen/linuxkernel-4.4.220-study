        "-I/usr/local/gcc-linaro-5.5.0-2017.10-x86_64_arm-linux-gnueabi/bin/../lib/gcc/arm-linux-gnueabi/5.5.0/include-fixed",
        "-I/usr/local/gcc-linaro-5.5.0-2017.10-x86_64_arm-linux-gnueabi/bin/../lib/gcc/arm-linux-gnueabi/5.5.0/../../../../arm-linux-gnueabi/include",
        "-I/usr/local/gcc-linaro-5.5.0-2017.10-x86_64_arm-linux-gnueabi/bin/../arm-linux-gnueabi/libc/usr/include"
    ],
    "clang.cflags": [
        "-std=c99",
        "-I${workspaceRoot}\\include",
        "-I${cwd}",
        "-I${workspaceFolder}/**",
        "-I${workspaceFolder}/include",
        "-I${workspaceFolder}/include/uapi/",
        "-I${workspaceFolder}/include/generated/uapi/",
        "-I${workspaceFolder}/arch/arm/include/",
        "-I${workspaceFolder}/arch/arm/include/uapi",
        "-I${workspaceFolder}/arch/arm/include/generated/",
        "-I${workspaceFolder}/arch/arm/include/generated/uapi",
        "-I/usr/local/gcc-linaro-5.5.0-2017.10-x86_64_arm-linux-gnueabi/bin/../lib/gcc/arm-linux-gnueabi/5.5.0/include",
        "-I/usr/local/gcc-linaro-5.5.0-2017.10-x86_64_arm-linux-gnueabi/bin/../lib/gcc/arm-linux-gnueabi/5.5.0/include-fixed",
